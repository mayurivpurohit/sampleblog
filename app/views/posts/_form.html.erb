  <%= form_for @post do |f|%>
  <%= f.hidden_field :user_id ,value: "#{current_user.id}" %>
    <strong><%= f.label :title %></strong><%= f.text_field :title %>
    <strong><%= f.label :content %></strong><%= f.text_area:content, :rows => 10, :cols => 5 %>
     <br/><%= f.submit "Submit", :class => 'btn btn-primary' %>
  <% end %>
